{
  "rewrites": [
    {
      "source": "/((?!assets/.*)(?!manifest.json)(?!service-worker.js)(?!logo.*)(?!screenshot.*)(?!vite.svg)(?!.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}